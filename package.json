{
  "name": "cypress-parallel-tests",
  "version": "1.0.0",
  "description": "Cypress e2e tests with best practices",
  "main": "index.js",
  "scripts": {
    "test": "node cypress/scripts/cypress.js  --reporter cypress-multi-reporters --reporter-options configFile=reporter-config.json",
    "cy:run:parallel":"run-p smoke:feedbackTests smoke:loginTests --print-label",
    "smoke:feedbackTests": "node cypress/scripts/cypress.js -s smoke/feedback --reporter cypress-multi-reporters --reporter-options configFile=reporter-config.json",
    "smoke:loginTests": "node cypress/scripts/cypress.js -s smoke/login  --reporter cypress-multi-reporters --reporter-options configFile=reporter-config.json",
    "cy:local:run": "ENVIRONMENT=local node cypress/scripts/cypress.js -b chrome",
    "cy:local:debug": "ENVIRONMENT=local TEST=smoke npx cypress open"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ISanjeevKumar/cypress-parallel-tests.git"
  },
  "keywords": [
    "cypress"
  ],
  "author": "Sanjeev Kumar",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ISanjeevKumar/cypress-parallel-tests/issues"
  },
  "homepage": "https://github.com/ISanjeevKumar/cypress-parallel-tests#readme",
  "dependencies": {
    "cypress": "6.1.0",
    "faker": "^5.5.3",
    "fs-extra": "^10.0.0",
    "mocha": "^9.1.3",
    "yargs": "^17.2.1"
  },
  "devDependencies": {
    "cypress-multi-reporters": "^1.5.0",
    "mocha-junit-reporter": "^2.0.2",
    "mochawesome": "^7.0.1",
    "mochawesome-merge": "^4.2.0",
    "mochawesome-report-generator": "^6.0.1",
    "npm-run-all": "^4.1.5"
  }
}
